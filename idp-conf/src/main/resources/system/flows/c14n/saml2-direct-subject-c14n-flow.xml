<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow.xsd"
      parent="SubjectC14N.Abstract">

    <!-- This is a one-step subflow that does NameID Subject Canonicalization. -->
    
    <action-state id="SAML2DirectSubjectCanonicalization">
        <evaluate expression="SAML2DirectCanonicalization" />
    </action-state>

    <bean-import resource="saml2-direct-subject-c14n-beans.xml" />

</flow>
