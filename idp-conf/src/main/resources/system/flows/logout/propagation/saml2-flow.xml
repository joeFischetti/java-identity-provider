<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow.xsd">

    <action-state id="PopulateLogoutPropagationContextWithResult">
        <evaluate expression="opensamlProfileRequestContext.getSubcontext(
            T(net.shibboleth.idp.session.context.LogoutPropagationContext)).setResultString('Failure')"
                  result="requestScope.logoutPropCtx" />
        <evaluate expression="'proceed'" />
        <transition on="proceed" to="proceed" />
    </action-state>

    <end-state id="proceed"/>

    <bean-import resource="saml2-beans.xml" />

</flow>
